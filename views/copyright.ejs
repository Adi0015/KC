            height: auto;
        }
        span img {
            display: block;
            text-align: center;
            font-size: 14px;
            line-height: 1.2;
            margin-top: 10px;
            max-width: 100%;
            height: auto;
        }
        .image-container label {
            display: block;
            text-align: center;
            font-size: 10px;
            line-height: 1.2;
            margin-top: 10px;
        }
        .logo-container {
            flex-direction: column;
            background-color: rgb(159, 159, 209);
            justify-content: center;
        }

        .logo-container img {
            max-width: 100%;
            height: auto;
        }
    }
