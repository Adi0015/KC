  width: 350px;
  border-radius: 3rem;
}
a{
    text-decoration: none;
    color: black;
    font-weight: bold;
    font-size: 20px;
}
a:hover{
    color: blue;
}
.image-container {
  flex-basis: calc(33.33% - 20px);
  padding: 10px;
  box-sizing: border-box;
  text-align: center;
}
.image-container img {
  height: 270px;
  width: 270px;
  border-radius:2rem;
}
table {
      width: 100%;
      border-collapse: collapse;
}
td {
      padding: 10px;
      text-align: center;
}
.logo-container {
        display: flex;
        flex-direction: wrap;
        align-items: center;
        text-align: center;
        
    }
@media only screen and (max-width: 600px) {
        .image-container img {
